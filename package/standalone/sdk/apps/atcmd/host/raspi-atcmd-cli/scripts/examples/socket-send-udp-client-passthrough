
ECHO "Run UDP server."
ECHO " - IP : 192.168.200.1"
ECHO " - Port : 50000"
HOLD

AT+SOPEN="udp",60000
AT+SLIST?

WAIT 1s

ECHO "Send the data. (length : 2048-byte, count: 1000)"
HOLD

AT+SSEND=0,"192.168.200.1",50000
LOOP 1 1000
DATA 2048

CALL socket-send-passthrough-exit

WAIT 2s

AT+SCLOSE
AT+SLIST?

